        <mxCell id="ShQKBhI2_hBmmikGelj6-13" value="&lt;div style=&quot;&quot; title=&quot;Page 2&quot; class=&quot;page&quot;&gt;&lt;div class=&quot;layoutArea&quot;&gt;&lt;div class=&quot;column&quot;&gt;&lt;div title=&quot;Page 2&quot; class=&quot;page&quot;&gt;&lt;div class=&quot;layoutArea&quot;&gt;&lt;div class=&quot;column&quot;&gt;&lt;div title=&quot;Page 2&quot; class=&quot;page&quot;&gt;&lt;div class=&quot;layoutArea&quot;&gt;&lt;div class=&quot;column&quot;&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;INSERT INTO preisinfo&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;VALUES(9999,NULL,2.99,3.99);&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;&lt;br&gt;&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;&lt;b&gt;Fehler:&lt;/b&gt;&amp;nbsp;&lt;font style=&quot;font-size: 13px;&quot;&gt;&lt;span style=&quot;caret-color: rgb(68, 68, 68);&quot;&gt;&lt;span style=&quot;color: rgb(68, 68, 68); font-family: monospace; background-color: rgb(255, 192, 203);&quot;&gt;Column &#39;bezeichnung&#39; cannot be null&lt;/span&gt;&lt;font face=&quot;monospace&quot;&gt; -&lt;/font&gt;&lt;font style=&quot;font-size: 13px;&quot; face=&quot;Times New Roman&quot;&gt;-&amp;gt;&amp;nbsp;Null Werte sind für dieses Feld nicht erlaubt&lt;/font&gt;&lt;/span&gt;&lt;/font&gt;&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;&lt;font style=&quot;font-size: 13px;&quot; face=&quot;Times New Roman&quot;&gt;Die Zeile&amp;nbsp;kann nicht eingefügt werden, da bei der&amp;nbsp;Erstellung der&amp;nbsp;Tabelle keine Null Werte erlaubt wurden.&lt;/font&gt;&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;&lt;font style=&quot;font-size: 13px;&quot; face=&quot;Times New Roman&quot;&gt;&lt;br&gt;&lt;/font&gt;&lt;/div&gt;&lt;div class=&quot;column&quot;&gt;&lt;font color=&quot;#030303&quot; style=&quot;font-size: 14px;&quot; face=&quot;monospace&quot;&gt;&lt;b&gt;Auszug aus CREATE TABLE Befehl:&lt;/b&gt;&lt;/font&gt;&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;&lt;div class=&quot;column&quot;&gt;CREATE TABLE artikel (&lt;/div&gt;&lt;div class=&quot;column&quot;&gt;anr int PRIMARY KEY,&lt;/div&gt;&lt;div class=&quot;column&quot;&gt;bezeichnung VARCHAR(255) &lt;span style=&quot;background-color: rgb(255, 10, 10);&quot;&gt;NOT NULL,&lt;/span&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;" style="whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;gradientDirection=west;align=left;spacingLeft=16;strokeWidth=1;gradientColor=#97d077;" vertex="1" parent="1">
          <mxGeometry x="24" y="576" width="636" height="168" as="geometry" />
        </mxCell>
        <mxCell id="ShQKBhI2_hBmmikGelj6-14" value="&lt;div style=&quot;&quot; title=&quot;Page 2&quot; class=&quot;page&quot;&gt;&lt;div class=&quot;layoutArea&quot;&gt;&lt;div class=&quot;column&quot;&gt;&lt;div title=&quot;Page 2&quot; class=&quot;page&quot;&gt;&lt;div class=&quot;layoutArea&quot;&gt;&lt;div class=&quot;column&quot;&gt;&lt;div title=&quot;Page 2&quot; class=&quot;page&quot;&gt;&lt;div class=&quot;layoutArea&quot;&gt;&lt;div class=&quot;column&quot;&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;&lt;b&gt;1) VIEW löschen:&lt;/b&gt;&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;DROP VIEW preisinfo&lt;br&gt;&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;&lt;b&gt;2) neue View erstellen:&lt;/b&gt;&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;CREATE VIEW Preisinfo AS SELECT anr, einstandspreis, listenpreis FROM artikel&lt;/span&gt;&lt;b&gt;&lt;br&gt;&lt;/b&gt;&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&lt;b&gt;3) Daten einfügen:&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;font-size: 13px;&quot; class=&quot;column&quot;&gt;&lt;div class=&quot;column&quot;&gt;INSERT INTO preisinfo&lt;/div&gt;&lt;div class=&quot;column&quot;&gt;VALUES(9999,2.99,3.99);&lt;/div&gt;&lt;div class=&quot;column&quot;&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;	&lt;/span&gt;Nicht möglich: Da kein DEFAULT Wert in der CREATE TABLE Anweisung vorgegeben wurde.&lt;/div&gt;&lt;div class=&quot;column&quot;&gt;&lt;br&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;" style="whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;gradientDirection=west;align=left;spacingLeft=16;strokeWidth=1;gradientColor=#97d077;" vertex="1" parent="1">
